<h3>Sorted Students based on Staus</h3>
@{
    List<Student> stds = (List<Student>)ViewData["slist"];
    foreach(Student s in stds){
        <li>
            @s.Student_Id" "@s.Student_Name" "@s.Student_Email_Id" "@s.Mobile_number" "@s.Student_address" "@s.Admission_date" "@s.Fees" "@s.Status
        </li>
    }
}
<a href="/Student/SortStudent">Display students</a>
