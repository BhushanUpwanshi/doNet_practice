<ul>
    @{
        List<Student> stds = (List<Student>)ViewData["sl"];
        foreach (Student s in stds)
        {
            <li>@s.Student_ID @s.Student_Name @s.Student_Email @s.Student_Address</li>
        }
    }
</ul>